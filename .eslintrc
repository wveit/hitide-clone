{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:jest/recommended",
        "plugin:react/recommended",
        "plugin:react/jsx-runtime"
    ],
    "plugins": ["jest"],
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "react/prop-types": false
    }
}
